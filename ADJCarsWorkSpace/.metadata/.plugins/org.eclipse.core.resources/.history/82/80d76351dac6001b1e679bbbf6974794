package dam.adjcars.model;

public class Empleado {
	private String passwordEmpleado;
	private String nombre;
	private String direccion;
	private String telefono;
	private String cargo;

	public Empleado() {
	}

	public Cliente(String passwordCliente, String nombre) {
		this.passwordEmpleado = passwordCliente;
		this.nombre = nombre;
	}

	public Cliente(String passwordCliente, String nombre, String direccion, String telefono, String cargo) {
		this.passwordEmpleado = passwordCliente;
		this.nombre = nombre;
		this.direccion = direccion;
		this.telefono = telefono;
		this.cargo = cargo;
	}

	public String getPasswordCliente() {
		return passwordEmpleado;
	}

	public String getNombre() {
		return nombre;
	}

	public String getDireccion() {
		return direccion;
	}

	public String getTelefono() {
		return telefono;
	}

	public String getCargo() {
		return cargo;
	}



}
